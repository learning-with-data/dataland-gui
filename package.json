{
  "name": "dataland-gui",
  "version": "0.3.0",
  "description": "Editor for the DataLand project",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --config-name=component --mode=production",
    "start": "webpack-dev-server --open --config-name=demo --mode=development",
    "lint": "eslint src test",
    "prepare": "npm run build",
    "pretest": "rm -rf .nyc_output || true && rm -rf reports || true && npm run lint",
    "test": "jest ----config jest.config.js ./test && cypress run --browser chrome --headless",
    "posttest": "npm run combine:reports",
    "cypress:open": "cypress open",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "nyc merge reports && mv coverage.json .nyc_output/out.json",
    "postcombine:reports": "nyc report"
  },
  "author": {
    "name": "Sayamindu Dasgupta",
    "email": "sayamindu@unc.edu"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@cypress/code-coverage": "^3.9.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^3.6.0",
    "cypress": "^4.12.1",
    "cypress-file-upload": "^4.1.1",
    "dayjs": "^1.10.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.17.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.22.0",
    "events": "^3.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^3.1.0",
    "webpack": "^4.45.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1",
    "webpack-node-externals": "^2.5.2"
  },
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "dayjs": "^1.8.29",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.2.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "blockly": "^3.20200924.4",
    "dataland-interpreter": "git+https://github.com/learning-with-data/dataland-interpreter.git#5816eb4124948cf1e437b7b2b5ab05ba45a45b89",
